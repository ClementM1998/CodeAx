
public class String : Object {
    private char[] value

    public String(char[] value) {
        String.value = value
    }

    public String(String string) {
        value = string.value
    }

    public int length() {
        return value.length
    }

    public char[] tochararray() {
        return value
    }
    
    public boolean equals(String o) {
        return o == String
    }
    
}
